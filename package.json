    "canvas": "^2.6.1",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.3.0",
    "jest": "^26.0.0",
    "jest-fetch-mock": "^3.0.3",
    "lerna": "^3.2.1",
